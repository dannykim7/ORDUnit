--------------------------------------------------------
--  DDL for Procedure BOARD_WRITE
--------------------------------------------------------
set define off;

  CREATE OR REPLACE PROCEDURE "JAVA00"."BOARD_WRITE" 
(
  V_TITLE IN board.title%TYPE 
, V_CONTENT IN board.content%TYPE  
, V_WRITER IN board.writer%TYPE 
) AS 
BEGIN
  INSERT INTO board(no, title, content, writer)
  values(board_seq.nextval, V_TITLE, V_CONTENT, V_WRITER);
  COMMIT;
END BOARD_WRITE;

/
--------------------------------------------------------
--  DDL for Procedure NOTICE_WRITE
--------------------------------------------------------
set define off;

  CREATE OR REPLACE PROCEDURE "JAVA00"."NOTICE_WRITE" 
(
  V_TITLE IN VARCHAR2 
, V_CONTENT IN VARCHAR2 
, V_STARTDATE IN VARCHAR2 
, V_ENDDATE IN VARCHAR2 
) AS 
BEGIN
  INSERT INTO notice(no, title, content, startDate, endDate)
  values(board_seq.nextval, V_TITLE, V_CONTENT, V_STARTDATE, V_ENDDATE);
  COMMIT;
END NOTICE_WRITE;

/
